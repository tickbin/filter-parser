{
  "name": "tickbin-filter-parser",
  "version": "0.0.1",
  "description": "parse tickbin string filters into filter functions",
  "main": "build/index.js",
  "scripts": {
    "clean": "rm -rf build/*",
    "test": "tape -r source-map-support/register 'build/test/**/*.js'",
    "build": "babel *.js **/*.js --source-maps --out-dir build/",
    "watch": "babel *.js **/*.js --source-maps --out-dir build/ --watch",
    "prebuild": "npm run clean",
    "prewatch": "npm run clean",
    "prepublish": "npm run build"

  },
  "author": "Jonathan Bowers <jonotron@gmail.com>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "source-map-support": "^0.4.0",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "chrono-node": "^1.2.1",
    "chrono-refiner-wholemonth": "0.0.4",
    "filtrex": "^0.5.4"
  }
}
